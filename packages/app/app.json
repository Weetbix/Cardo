{
  "name": "Cardo",
  "displayName": "Cardo",
  "expo": {
    "name": "Cardo",
    "description": "Cardo helps you write heartfelt messages to friends and family by providing you with examples to expand upon.",
    "slug": "cardo",
    "privacy": "unlisted",
    "version": "1.0.2",
    "entryPoint": "node_modules/expo/AppEntry.js",
    "platforms": [
      "android"
    ],
    "assetBundlePatterns": [
      "src/**/*.png",
      "src/data/images/*",
      "assets/*"
    ],
    "primaryColor": "#FFFFFF",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png"
    },
    "android": {
      "package": "com.johnhannagan.cardo",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-adaptive-foreground.png"
      },
      "permissions": []
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#FFFFFF"
    },
    "githubUrl": "https://github.com/Weetbix/cardo"
  }
}
